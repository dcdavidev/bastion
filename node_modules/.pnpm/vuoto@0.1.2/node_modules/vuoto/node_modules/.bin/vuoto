#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules/vuoto/dist/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules/vuoto/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules/vuoto/dist/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules/vuoto/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/vuoto@0.1.2/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
