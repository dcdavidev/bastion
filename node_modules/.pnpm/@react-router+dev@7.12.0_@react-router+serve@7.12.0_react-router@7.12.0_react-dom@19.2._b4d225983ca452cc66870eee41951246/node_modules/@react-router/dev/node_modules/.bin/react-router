#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules/@react-router/dev/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules/@react-router/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules/@react-router/dev/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules/@react-router/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/@react-router+dev@7.12.0_@react-router+serve@7.12.0_react-router@7.12.0_react-dom@19.2._b4d225983ca452cc66870eee41951246/node_modules:/home/dcdavidev/bastion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
